/boot	emmc	/dev/block/mmcblk0p1
/cache	ext4	/dev/block/mmcblk0p15
/data	ext4	/dev/block/mmcblk0p19
/factory	emmc	/dev/block/mmcblk0p12	flags=display="Factory";backup=1
/misc	emmc	/dev/block/mmcblk0p13	flags=display="Misc";backup=1
/recovery	emmc	/dev/block/mmcblk0p2	flags=display="Recovery";backup=1
/splashscreen	emmc	/dev/block/mmcblk0p4	flags=display="Splashscreen";backup=1
/system	ext4	/dev/block/mmcblk0p18
/external_sd	vfat	/dev/block/mmcblk1p1	flags=display="MicroSD";storage;wipeingui;removable
